{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "主窗口权限配置",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-write-file",
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
