{
  "build": {
    "beforeDevCommand": "pnpm web:dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:6408"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "wix": {
        "language": ["zh-CN"]
      },
      "nsis": {
        "languages": ["SimpChinese"]
      },
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "publisher": "Zihan Hu",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "ExCaller",
  "version": "0.4.2",
  "identifier": "app.is-an.ex-caller",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ExCaller",
        "width": 800,
        "height": 600,
        "center": true
      }
    ]
  }
}
